<?xml version="1.0" encoding="UTF-8"?>
<!-- 电影数据源配置文件 -->
<configuration>
    <!-- 数据源配置部分 -->
    <datasources>
        <!-- 测试数据源 -->
        <datasource id="test" class="com.example.custom.CustomMovieService">
            <name>云云TV数据源</name>
            <description>从云云TV网站获取电影数据</description>
        </datasource>
    </datasources>

    <!-- URL映射配置部分 -->
    <urlMappings>
        <!-- URL到数据源的映射关系 -->
        <urlMapping baseUrl="https://www.test.tv" datasource="test"/>

        <!-- 通配符匹配，作为默认数据源 -->
        <urlMapping baseUrl="*" datasource="test_demo"/>
    </urlMappings>
</configuration>